---
import BaseLayout from '../layouts/BaseLayout.astro';

const structuredData = [
  {
    "@context": "https://schema.org",
    "@type": "ImageGallery",
    "name": "Kazikowy Festiwal Muzyczny",
    "description": "Galeria zdjęć z Kazikowego Festiwalu Muzycznego",
    "url": "https://jankobus.com/kazik_festiwal.html",
    "creator": {
      "@type": "Person",
      "name": "Jan Kobus"
    },
    "datePublished": "2025-01-01"
  },
  {
    "@context": "https://schema.org",
    "@type": "BreadcrumbList",
    "itemListElement": [{
      "@type": "ListItem",
      "position": 1,
      "name": "Strona główna",
      "item": "https://jankobus.com/"
    },{
      "@type": "ListItem",
      "position": 2,
      "name": "Fotografia Eventowa",
      "item": "https://jankobus.com/events.html"
    },{
      "@type": "ListItem",
      "position": 3,
      "name": "Kazikowy Festiwal Muzyczny",
      "item": "https://jankobus.com/kazik_festiwal.html"
    }]
  }
];
---

<BaseLayout
    title="Kazikowy Festiwal Muzyczny - Galeria Zdjęć | Jan Kobus Fotografia"
    description="Galeria zdjęć z Kazikowego Festiwalu Muzycznego. Profesjonalna fotografia eventowa - Jan Kobus."
    keywords="Kazikowy Festiwal, fotografia festiwalowa, zdjęcia eventowe, Jan Kobus, fotografia koncertowa"
    canonical="https://jankobus.com/kazik_festiwal"
    ogType="article"
    ogImage="https://jankobus.com/photos/kazik%20festiwal/foto%20(1).jpg"
    twitterCard="summary_large_image"
    structuredData={structuredData}
    preloadImages={["/photos/kazik%20festiwal/foto%20(1).jpg"]}
>

    <nav>
        <div class="nav-left">
            <a href="/#work">Prace</a>
            <a href="/o-mnie">O mnie</a>
            <a href="/kontakt">Kontakt</a>
        </div>
        <div class="nav-center">
            <a href="/">Jan Kobus</a>
        </div>
        <div class="nav-right">
            <a href="https://www.facebook.com/profile.php?id=100069116356707" target="_blank"><i class="fab fa-facebook-f"></i></a>
            <a href="https://www.instagram.com/redix.45/" target="_blank"><i class="fab fa-instagram"></i></a>
        </div>
    </nav>

    <header class="project-header">
        <h1>Kazikowy Festiwal Muzyczny</h1>
    </header>

    <section class="masonry-gallery" id="moja-galeria">
    </section>

    <div class="see-more-container">
        <h2 class="see-more-title">Zobacz więcej albumów</h2>
        <div class="related-albums">
            <div class="gallery-container">
                <div class="gallery-item">
                    <a href="/operetka">
                        <picture>
                            <source srcset="/photos/operetka/foto%20(2).webp" type="image/webp">
                            <source srcset="/photos/operetka/foto%20(2).jpg">
                            <img src="/photos/operetka/foto%20(2).jpg" alt="Fotografia eventowa Lubliniec - koncert Cabaret w Operetce" loading="eager" decoding="async">
                        </picture>
                    </a>
                </div>
                <div class="caption-box">
                    <div class="caption-title">Cabaret w Operetce</div>
                    <div class="caption-year">2025</div>
                </div>
            </div>
            <div class="gallery-container">
                <div class="gallery-item">
                    <a href="/dzien_niepodleglosci">
                        <picture>
                            <source srcset="/photos/dzien_niepodleglosci_kazik2025/foto%20(1).webp" type="image/webp">
                            <source srcset="/photos/dzien_niepodleglosci_kazik2025/foto%20(1).jpg">
                            <img src="/photos/dzien_niepodleglosci_kazik2025/foto%20(1).jpg" alt="Fotografia eventowa Lubliniec - Dzien Niepodleglosci ZSOT Kazik" loading="eager" decoding="async">
                        </picture>
                    </a>
                </div>
                <div class="caption-box">
                    <div class="caption-title">Dzień Niepodległości ZSOT</div>
                    <div class="caption-year">2025</div>
                </div>
            </div>
        </div>
    </div>

    <footer>
        <div class="footer-content">
            <p>&copy; 2026 Jan Kobus. Wszelkie prawa zastrzeżone.</p>
            <div class="footer-socials">
                <a href="https://www.facebook.com/profile.php?id=100069116356707" target="_blank"><i class="fab fa-facebook-f"></i></a>
                <a href="https://www.instagram.com/redix.45/" target="_blank"><i class="fab fa-instagram"></i></a>
            </div>
        </div>
    </footer>

    <script src="/script.js" is:inline></script>
    <script>
        loadGallery({
            containerId: 'moja-galeria',
            folder: '/photos/kazik festiwal/',
            count: 18,
            prefix: 'foto',
            extension: '.jpg',
            altTemplate: 'Fotografia eventowa Lubliniec Kazikowy Festiwal Muzyczny zdjęcia koncertowe'
        });
    </script>

</BaseLayout>
