---
import BaseLayout from '../layouts/BaseLayout.astro';

const structuredData = [
  {
    "@context": "https://schema.org",
    "@type": "ImageGallery",
    "name": "Cabaret w Operetce",
    "description": "Galeria zdjęć z koncertu Cabaret w Operetce",
    "url": "https://jankobus.com/operetka",
    "creator": {
      "@type": "Person",
      "name": "Jan Kobus"
    },
    "datePublished": "2025-01-01"
  },
  {
    "@context": "https://schema.org",
    "@type": "BreadcrumbList",
    "itemListElement": [{
      "@type": "ListItem",
      "position": 1,
      "name": "Strona główna",
      "item": "https://jankobus.com/"
    },{
      "@type": "ListItem",
      "position": 2,
      "name": "Fotografia Eventowa",
      "item": "https://jankobus.com/events"
    },{
      "@type": "ListItem",
      "position": 3,
      "name": "Cabaret w Operetce",
      "item": "https://jankobus.com/operetka"
    }]
  }
];
---

<BaseLayout
    title="Cabaret w Operetce - Galeria Zdjęć | Jan Kobus Fotografia"
    description="Galeria zdjęć z koncertu Cabaret w Operetce. Profesjonalna fotografia eventowa - Jan Kobus."
    keywords="Cabaret, Operetka, fotografia koncertowa, zdjęcia eventowe, Jan Kobus"
    canonical="https://jankobus.com/operetka"
    ogType="article"
    ogImage="https://jankobus.com/photos/operetka/foto%20(1).jpg"
    twitterCard="summary_large_image"
    structuredData={structuredData}
    preloadImages={["/photos/operetka/foto%20(1).jpg"]}
>

    <nav>
        <div class="nav-left">
            <a href="/#work">Prace</a>
            <a href="/o-mnie">O mnie</a>
            <a href="/kontakt">Kontakt</a>
        </div>
        <div class="nav-center">
            <a href="/">Jan Kobus</a>
        </div>
        <div class="nav-right">
            <a href="https://www.facebook.com/profile.php?id=61587671346611" target="_blank"><i class="fab fa-facebook-f"></i></a>
            <a href="https://www.instagram.com/redix.45/" target="_blank"><i class="fab fa-instagram"></i></a>
        </div>
    </nav>

    <header class="project-header">
        <h1>Operetka</h1>
    </header>

    <section class="masonry-gallery" id="moja-galeria">
    </section>

    <div class="see-more-container">
        <h2 class="see-more-title">Zobacz więcej albumów</h2>
        <div class="related-albums">
            <div class="gallery-container">
                <div class="gallery-item">
                    <a href="/dzien_niepodleglosci">
                        <picture>
                            <source srcset="/photos/dzien_niepodleglosci_kazik2025/foto%20(1).webp" type="image/webp">
                            <source srcset="/photos/dzien_niepodleglosci_kazik2025/foto%20(1).jpg">
                            <img src="/photos/dzien_niepodleglosci_kazik2025/foto%20(1).jpg" alt="Fotografia eventowa Lubliniec - Dzien Niepodleglosci ZSOT Kazik" loading="eager" decoding="async">
                        </picture>
                    </a>
                </div>
                <div class="caption-box">
                    <div class="caption-title">Dzień Niepodległości ZSOT</div>
                    <div class="caption-year">2025</div>
                </div>
            </div>
            <div class="gallery-container">
                <div class="gallery-item">
                    <a href="/koszecin_jarmark">
                        <picture>
                            <source srcset="/photos/koszecin_jarmark/foto%20(4).webp" type="image/webp">
                            <source srcset="/photos/koszecin_jarmark/foto%20(4).jpg">
                            <img src="/photos/koszecin_jarmark/foto%20(4).jpg" alt="Fotografia eventowa Koszecin - jarmark lokalny blisko Lublinca" loading="eager" decoding="async">
                        </picture>
                    </a>
                </div>
                <div class="caption-box">
                    <div class="caption-title">Koszęcin Jarmark</div>
                    <div class="caption-year">2025</div>
                </div>
            </div>
        </div>
    </div>

    <footer>
        <div class="footer-content">
            <p>&copy; 2026 Jan Kobus. Wszelkie prawa zastrzeżone.</p>
            <div class="footer-socials">
                <a href="https://www.facebook.com/profile.php?id=61587671346611" target="_blank"><i class="fab fa-facebook-f"></i></a>
                <a href="https://www.instagram.com/redix.45/" target="_blank"><i class="fab fa-instagram"></i></a>
            </div>
        </div>
    </footer>

    <script src="/script.js" is:inline></script>
    <script>
        loadGallery({
            containerId: 'moja-galeria',
            folder: '/photos/operetka/',
            count: 44,
            prefix: 'foto',
            extension: '.jpg',
            altTemplate: 'Fotografia eventowa Lubliniec koncert Cabaret w Operetce zdjęcia sceniczne'
        });
    </script>

</BaseLayout>
